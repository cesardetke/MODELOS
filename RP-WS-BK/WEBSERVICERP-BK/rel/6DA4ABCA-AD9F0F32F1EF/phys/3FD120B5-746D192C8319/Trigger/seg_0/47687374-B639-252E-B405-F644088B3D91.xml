<body><![CDATA[BEGIN

     INSERT INTO RAPIPAGO.RP_AGENTE_SERVICE_CHARGE (ID_AGENTE_SERVICE_CHARGE, 
                                                    ID_AGENTE, 
                                                    IMPORTE_TOPE, 
                                                    CANTIDAD_MAXIMA, 
                                                    IS_DELETED, 
                                                    ID_ENV)
                                                    SELECT
                                                    (TO_CHAR(SYSDATE,'YYMMDDHH24MI')||dbms_random.string('A', 10)), 
                                                    :NEW.ID_AGENTE, 
                                                    600, 
                                                    999999, 
                                                    'N', 
                                                    'RPPRI'
                                                    FROM DUAL
                                                    WHERE NOT EXISTS (SELECT ID_AGENTE 
                                                                      FROM RAPIPAGO.RP_AGENTE_SERVICE_CHARGE 
                                                                      WHERE ID_AGENTE = :NEW.ID_AGENTE);
EXCEPTION
     WHEN OTHERS THEN
       -- 
       RAPIPAGO.RP_FUNCIONES_UTILES.LOG_PROCESO_CENTRAL( $$PLSQL_UNIT, 'Error en ' || $$PLSQL_UNIT || ': ' || SQLERRM, 'S');
       RAISE ;
END;]]></body>
